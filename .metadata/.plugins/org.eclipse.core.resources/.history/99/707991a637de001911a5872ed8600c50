package io;

import org.springframework.beans.factory.annotation.Required;

public class EMp {

	
	
	private String  name;
	public String getName() {
		return name;
	}
        @Required	
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	private int age;
}
