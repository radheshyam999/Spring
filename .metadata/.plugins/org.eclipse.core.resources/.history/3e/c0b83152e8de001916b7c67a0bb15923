package io;

public class B extends A{

	public void show() {
		System.out.println("child class");
	}
	
	public static void main(String... s) {
		
	    A a =(A) new B();
	     a.letLearn();
	    B b = (B) new A();
	    
		b.show();
		
//		b.letLearn();
//        b = (A)new A();	    	
