package haver;

public class Emp {
   private int ta;
   private int da;
   private int basic;
   public int getTa() {
	  
	   return ta;
      }
   public void setTa(int ta) {
	  
	   this.ta = ta;
      }
   public int getDa() {
	   return da;
     }
   public void setDa(int da) {
	this.da = da;
      }
   public int getBasic() {
	 return basic;
     }
   public void setBasic(int basic) {
	  this.basic = basic;
     }
	
	
	
	
	
	
}
