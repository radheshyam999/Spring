/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
    	 ApplicationContext context = new ClassPathXmlApplicationContext("");
    	 Emp emp1 = (Emp) context.getBean("NewFile.xml");
    	 emp1.show()
         
    
    }
}
